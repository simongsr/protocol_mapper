model User {

    enum Pets {
        DOG     = "dog",
        CAT     = "cat",
        HAMSTER = "ham"
    }

    model UserProfile {
        optional string role = 21 [default = ''];
    }

    model Friend {
        required string name  = 31;
        optional date   since = 32;
    }

    required string           name    = 1 [python_django__unique];
    required int              age     = 2 [half = 0.5];
    optional string           address = 3 [default = "nonloso"] [max_length = 50];
    required User.UserProfile profile = 4 [default = User.Pets.DOG];
    repeated string           words   = 5;
    repeated User.Friend    friends = 6;
    # repeated User.Pets        pets    = 7;
}

